    ]
  },
  "dependencies": {
    "@navvi/react-use-metamask": "^0.1.14",
    "browser-tabs-lock": "^1.2.15",
    "es-cookie": "^1.3.2",
    "promise-polyfill": "8.2.3",
    "uuid": "^8.3.2"
  }
